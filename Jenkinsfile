pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Build') {
            steps {
                echo 'This is build stage'
            }
        }
        stage('Dev') {
            steps {
                echo 'This is dev stage'
            }
        }
        stage('Test') {
            steps {
                echo 'This is test stage'
            }
        }
        stage('Release') {
            steps {
                echo 'This is release stage'
            }
        }
    }
}
